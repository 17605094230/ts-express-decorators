{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "noImplicitAny": false,
    "sourceMap": true,
    "declaration":false,
    "experimentalDecorators":true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "node",
    "isolatedModules": false,
    "noLib": false,
    "suppressImplicitAnyIndexErrors": false,
    "lib": ["es6"]
  },

  "exclude": [
    "node_modules",
    "models",
    "test/old",
    "typings"
  ]
}