{
  "name": "ts-express-decorators",
  "version": "0.3.1",
  "description": "Use TypeScript decorators to create your Express application and Rest API",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run tsc:es5 && npm run tsc:es6",
    "coverage": "npm run test-cov && npm run test-remap",
    "postinstall": "npm run build",
    "tslint": "tslint ./*.ts ./lib/*.ts",
    "test": "mocha --reporter spec --check-leaks --bail test/",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "test:cov": "istanbul cover -x \"examples/**\" node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "test:remap": "./node_modules/.bin/remap-istanbul -i coverage/coverage.json -o coverage/lcov-report -t html",
    "test:travis": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "tsc": "tsc",
    "tsc:es5": "tsc --project tsconfig.json",
    "tsc:es6": "tsc --project tsconfig.es6.json"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/romakita/ts-express-decorators.git"
  },
  "keywords": [
    "Express",
    "TypeScript",
    "Decorator",
    "decorators",
    "decorator",
    "typescript",
    "express",
    "Controller",
    "Inject",
    "ioc",
    "ES2015",
    "ES6"
  ],
  "author": "Romain Lenzotti",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/romakita/ts-express-decorators/issues"
  },
  "homepage": "https://github.com/romakita/ts-express-decorators#readme",
  "dependencies": {
    "@types/bluebird": "^3.0.33",
    "@types/chai": "^3.4.32",
    "@types/cookie-parser": "^1.3.29",
    "@types/express": "^4.0.32",
    "@types/express-serve-static-core": "^4.0.33",
    "@types/hashmap": "^2.0.28",
    "@types/mocha": "^2.2.31",
    "@types/supertest": "^1.1.30",
    "bluebird": "^3.3.1",
    "glob": "^7.0.3",
    "hashmap": "^2.0.6",
    "source-map-support": "^0.4.0",
    "ts-httpexceptions": "^2.0.3",
    "ts-log-debug": "^2.0.1"
  },
  "peerDependencies": {
    "express": "^4"
  },
  "devDependencies": {
    "body-parser": "^1.15.1",
    "chai": "^3.5.0",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "istanbul": "^0.4.2",
    "method-override": "^2.3.5",
    "mocha": "^2.4.5",
    "morgan": "^1.7.0",
    "remap-istanbul": "^0.6.4",
    "supertest": "^1.2.0",
    "tslint": "^3.5.0",
    "typescript": "^2.0.2"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
