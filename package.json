  "bugs": {
    "url": "https://github.com/romakita/express-router-decorator/issues"
  },
  "homepage": "https://github.com/romakita/express-router-decorator#readme",
  "dependencies": {
    "bluebird": "^3.3.1",
    "httpexceptions": "^1.2.1",
    "lodash": "^4.5.0",
    "log-debug": "^1.1.4",
    "source-map-support": "^0.4.0",
    "typescript": "^1.7.5"
  },
  "direcories": {
    "lib": "lib",
    "test": "test"
  },
  "devDependencies": {
    "chai": "^3.5.0",
