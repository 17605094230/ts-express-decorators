{
  "name": "express-router-decorator",
  "version": "1.0.0",
  "description": "Express Router decorator",
  "main": "lib/index.js",
  "scripts": {
    "prepublish":"typings"
    "beforeinstall":"typings install",
    "afterinstall":"tsc",
    "beforetest":"tsc",
    "test": "mocha --reporter spec --check-leaks --bail test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "test-travis": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/romakita/express-router-decorator.git"
  },
  "keywords": [
    "Express",
